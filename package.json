{
  "name": "graphql-backend-boilerplate",
  "version": "1.0.0",
  "description": "Graphql back-end service boilerplate based on node and typescript.",
  "main": "dist/index.js",
  "repository": "https://github.com/guog/graphql-backend-boilerplate.git",
  "author": "guog <guog@live.cn>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.2.4",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "prisma": "^3.4.2",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@prisma/client": "^3.4.2",
    "apollo-server-core": "^3.5.0",
    "apollo-server-express": "^3.5.0",
    "dotenv": "^10.0.0",
    "graphql": "^16.0.1",
    "graphql-scalars": "^1.13.1",
    "nexus": "^1.1.0"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "lint-staged": {
    "src/**/*.{ts,md}": [
      "npx prettier --write --ignore-unknown",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "prepare": "husky install"
  }
}
